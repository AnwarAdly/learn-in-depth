#include <stdio.h>
void main(){
	int m=29;
	printf("Address of m : %p \nValue of m : %d",&m,m);
	int* ab=&m;
	printf("Now ab is assigned with the address of m.");
	printf("Address of ab : %p \nValue of ab : %d",ab,*ab);
	m=34;
	printf("Now the value of m assigned to 34");
	printf("Address of ab : %p \nValue of ab : %d",ab,*ab);
	*ab=7;
	printf("Now the value of ab assigned to 7");
	printf("Address of m : %p \nValue of m : %d",&m,m);
}
