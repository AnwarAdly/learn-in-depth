#include <stdio.h>

int uniqueNo (int x[5]);

void main (){
	int arr[5]={4,3,4,5,3};
	printf("Input : %d --> Numbers of ones in binary : %d",arr,uniqueNo(arr));

}

int uniqueNo (int x[5]){
	int i,uni;
	for(i=0;i<5;i++){
		if(x[i]==x[i+1])
			uniqueNo(x[i]);
		else
			uni=x[i];
	}
	return uni;
}
