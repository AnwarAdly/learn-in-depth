#include <stdio.h>

int uniqueNo (int x);

void main (){
	int arr[5]={4,3,4,5,3};
	printf("Input : {4,3,4,5,3} --> Unique number in array : %d",uniqueNo(arr[0]));

}

int uniqueNo (int x){
	int i,uni;
	for(i=0;i<5;i++){
		if(x[i]==x[i+1])
			uniqueNo(x[i]);
		else
			uni=x[i];
	}
	return uni;
}
