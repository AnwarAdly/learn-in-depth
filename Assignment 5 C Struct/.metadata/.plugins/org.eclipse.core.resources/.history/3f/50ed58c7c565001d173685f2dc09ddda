	printf("Enter imag : ");
	fflush(stdout);
	scanf("%f",&c1.imag);
	printf("Enter second complex number : ");
	printf("\nEnter real : ");
	fflush(stdout);
	scanf("%f",&c2.real);
	printf("Enter imag : ");
	fflush(stdout);
	scanf("%f",&c2.imag);
	sum.feet=d1.feet+d2.feet;
	sum.inch=(d1.inch+d2.inch)*0.083;
